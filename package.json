{
  "name": "@atmtfy/background-video",
  "version": "0.0.1",
  "description": "Automatic background video from various sources (Youtube, MP4, vimeo) with autoplay across devices. No JS dependencies.",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
    "build:demo": "cp -R dist demo"
  },
  "repository": {
    "type": "git",
    "url": "@atmtfy/background-video"
  },
  "keywords": [
    "background",
    "video",
    "autoplay",
    "youtube",
    "vanilla",
    "vimeo",
    "local",
    "mp4",
    "background",
    "hero"
  ],
  "author": "atmtfy",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.9",
    "typescript": "^4.5.4"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  }
}
